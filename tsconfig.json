{
  "include": [
    "src/Book/index.tsx"
  ],
  "compilerOptions": {
    "lib": [
      "dom",
      "es2015"
    ],
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "jsx": "preserve",
    "target": "es6",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
  }
}
